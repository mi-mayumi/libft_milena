----------------------------------------------------------------------------

putchar

int	main(void)
{
	ft_putchar_fd('a', 1);
	return (0);
}

----------------------------------------------------------------------------

putstr

int	main(void)
{
	ft_putstr_fd("Ola", 1);
	return (0);
}

----------------------------------------------------------------------------

strlen

#include <stdio.h>

int main(void)
{
	printf("Valor do i deve ser 7 = %d\n", ft_strlen("Lobinho"));
	return (0);
}

----------------------------------------------------------------------------

putendl

int	main(void)
{
	ft_putendl_fd("Ola", 1);
	return (0);
}

----------------------------------------------------------------------------

isalpha

#include <stdio.h>

int	main()
{
	printf("nao é uma letra: %d\n", ft_isalpha('#'));
	printf("é uma letra: %d\n", ft_isalpha('j'));
	return (0);
}

----------------------------------------------------------------------------

isdigit

#include <stdio.h> 

int	main(void)
{
	printf("é numero = %d\n", ft_isdigit('5'));
	printf("nao retorna nada = %d\n", ft_isdigit('a'));
}

----------------------------------------------------------------------------

is ascii

#include <stdio.h>

int main()
{
	printf("retorna zero: %d\n", ft_isascii(65));
	printf("retorna o num ascii: %d", ft_isascii(10));
	return (0);
}

----------------------------------------------------------------------------

isalnum

#include <stdio.h>

int main()
{
	printf("é alfanumérico: %d\n", ft_isalnum('1'));
	printf("é alfanumérico: %d\n", ft_isalnum('a'));
	printf("imprima zero: %d\n", ft_isalnum('#'));
	return (0);
}

----------------------------------------------------------------------------

isprint

#include <stdio.h>

int main()
{
	printf("printável: %d\n", ft_isprint(' '));
	printf("inprintável: %d\n", ft_isprint('\0'));
	printf("inprintável: %d\n", ft_isprint(5));
	return (0);
}

----------------------------------------------------------------------------

tolower

#include <stdio.h>

int main(void)
{
	printf("A retorna 97(a) = %d\n", ft_tolower('A'));
	return (0);
}

----------------------------------------------------------------------------

toupper 

#include <stdio.h>

int main(void)
{
	printf("A retorna 65(A) = %d\n", ft_toupper('a'));
	return (0);
}

----------------------------------------------------------------------------

strncmp

#include <stdio.h>

int	main(void)
{
	char s1[] = "Hello World";
	char s2[] = "HELLO";
	int n = 4;

	printf("%d", ft_strncmp(s1, s2, n));
}

----------------------------------------------------------------------------

strchr

#include <stdio.h>

int	main(void)
{
	char s[] = "Hello";
	int c = 'l';

	printf("%s\n", ft_strchr(s, c));
	return (0);
}
----------------------------------------------------------------------------

strrchr

#include <stdio.h>

int	main(void)
{
	char s[] = "Hello";
	int c = 'l';

	printf("%s\n", ft_strrchr(s, c));
	return (0);
}

----------------------------------------------------------------------------

atoi 

#include <stdio.h>

int main()
{
    char *str = " 	-123ab";
    printf("%d\n", ft_atoi(str));
    return 0;
}

----------------------------------------------------------------------------

ft_putnbr

#include <stdio.h>

int	main(void)
{
	ft_putnbr_fd(123, 1);
	return (0);
}

